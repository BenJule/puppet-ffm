# this file is managed by puppet

ddns-update-style none;

default-lease-time 600;
max-lease-time 7200;

log-facility local7;

subnet <%= @subnet %> netmask <%= @netmask %> {
  authoritative;
  range <%= @range_start %> <%= @range_end %>;
  option domain-name-servers <%= @dns_servers.join(', ') %>;
  option routers <%= @routers %>;
}
